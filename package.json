{
	"name": "smitedle",
	"private": true,
	"version": "0.0.5",
	"packageManager": "pnpm@8.3.1",
	"scripts": {
		"build": "run-s build:createSession build:next",
		"build:createSession": "node ./scripts/createSession.js",
		"build:next": "next build",
		"build:analyze": "cross-env ANALYZE=true npm run build",
		"clean": "git clean --force -dx --exclude node_modules/ --exclude .env.local --exclude .env.*.local",
		"dev": "run-s build:createSession dev:next",
		"dev:next": "next dev",
		"https": "local-ssl-proxy --source 3001 --target 3000",
		"devs": "npm-run-all --parallel https dev",
		"fix": "run-s fix:*",
		"fix:eslint": "npm run lint:eslint -- --fix",
		"fix:prettier": "npm run lint:prettier -- --write",
		"lint": "run-p lint:*",
		"lint:eslint": "eslint --ext .js,.ts,.tsx --ignore-path ./.eslintignore .",
		"lint:prettier": "prettier --check --ignore-path ./.prettierignore .",
		"lint:typescript": "tsc --noEmit",
		"start": "next start"
	},
	"dependencies": {
		"@joshmiquel/hirez": "^3.0.1",
		"@next/bundle-analyzer": "^13.4.5",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tsconfig/next": "^1.0.5",
		"autoprefixer": "^10.4.14",
		"clsx": "^1.2.1",
		"framer-motion": "^10.12.16",
		"fuzzyjs": "^5.0.1",
		"hirez.js": "^2.0.1",
		"next": "13.4.4",
		"next-compose-plugins": "^2.2.1",
		"next-pwa": "5.6.0",
		"postcss-import": "^15.1.0",
		"postcss-preset-env": "^8.4.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.9.0",
		"react-timer-hook": "^3.0.6",
		"sass": "^1.63.4",
		"seedrandom": "^3.0.5",
		"tailwind-merge": "^1.13.1",
		"tailwindcss": "^3.3.2"
	},
	"devDependencies": {
		"@tsconfig/next": "^1.0.3",
		"@types/node": "^18.16.18",
		"@types/react": "^18.2.12",
		"@types/react-dom": "^18.2.5",
		"@types/seedrandom": "^3.0.5",
		"@types/umami": "^0.1.1",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"cross-env": "^7.0.3",
		"eslint": "^8.42.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-next": "^13.4.5",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-etc": "^2.0.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-tailwindcss": "3.11.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.8",
		"prettier-plugin-tailwindcss": "^0.2.8",
		"typescript": "^5.1.3",
		"typescript-plugin-css-modules": "^5.0.1"
	}
}
